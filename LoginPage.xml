<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="425dp"
        android:layout_height="731dp"
        android:background="#8BE78B"
        app:srcCompat="@drawable/light_green_color_solid_background_1920x1080"
        tools:layout_editor_absoluteX="-7dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="422dp"
        android:layout_height="144dp"
        android:background="#008640"
        android:foregroundTint="#FFC107"
        android:translationZ="500pt"
        app:srcCompat="@drawable/flag_of_libya_19772011_svg"
        tools:layout_editor_absoluteX="-5dp"
        tools:layout_editor_absoluteY="644dp" />


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="411dp"
        android:layout_height="72dp"
        android:background="#008640"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView4" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="405dp"
        android:layout_height="34dp"
        android:fontFamily="casual"
        android:text="WELCOME TO FOODLESS APP"
        android:textSize="25sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="27dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="468dp"
        android:layout_height="229dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="174dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.149"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:srcCompat="@drawable/coll" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="350dp"
        android:layout_height="27dp"
        android:fontFamily="cursive"
        android:text="Buy less, Choose well, Make it last!!"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="311dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="93dp"
        android:layout_height="38dp"
        android:text="Name:"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="436dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="80dp"
        android:layout_height="37dp"
        android:text="Email Id:"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="474dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="149dp"
        android:layout_height="47dp"
        android:text="Date of Birth:"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="511dp" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="57dp"
        android:layout_height="55dp"
        android:translationZ="500pt"
        app:srcCompat="@drawable/recipe_book"
        tools:layout_editor_absoluteX="304dp"
        tools:layout_editor_absoluteY="660dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:fontFamily="monospace"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="429dp" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:fontFamily="monospace"
        android:inputType="textPersonName"
        android:text="Email ID"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="466dp" />

    <EditText
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:fontFamily="monospace"
        android:inputType="textPersonName"
        android:text="Date of Birth"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="501dp" />

    <ImageView
        android:id="@+id/listbutton"
        android:layout_width="66dp"
        android:layout_height="87dp"
        android:translationZ="600pt"
        app:srcCompat="@drawable/noun_list_5079140"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="651dp" />

    <ImageView
        android:id="@+id/plusbutton"
        android:layout_width="64dp"
        android:layout_height="49dp"
        android:translationZ="600pt"
        app:srcCompat="@drawable/noun_plus_sign_2826153"
        tools:layout_editor_absoluteX="173dp"
        tools:layout_editor_absoluteY="666dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="238dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/loginnnn"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="350dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="209dp"
        android:layout_height="73dp"
        app:srcCompat="@drawable/sign"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="546dp" />


</androidx.constraintlayout.widget.ConstraintLayout>
